adding: 0;\r\n  margin: 0;\r\n}\r\n\r\n*,\r\n*::before,\r\n*::after {\r\n  box-sizing: border-box;\r\n}\r\n\r\nbody {\r\n  width: 100%;\r\n  height: 100vh;\r\n}\r\n\r\n.page-container {\r\n  width: 100%;\r\n  height: 100%;\r\n  background-image: url('../images/background.jpg');\r\n  background-repeat: no-repeat;\r\n  background-size: cover;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n\r\n.layout-container {\r\n  width: 70%;\r\n  height: 100%;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  justify-content: flex-start;\r\n  background-color: rgba($color: #000000, $alpha: 0.3);\r\n}\r\n\r\n.steps-container {\r\n  padding: 60px 0 40px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n}\r\n\r\n.steps__title {\r\n  font-size: 30px;\r\n  color: $white;\r\n  font-family: 'Montserrat-Regular', sans-serif;\r\n}\r\n\r\nselect::-ms-expand {\r\n  display: none;\r\n}\r\nselect {\r\n  margin-top: 20px;\r\n  display: inline-block;\r\n  box-sizing: border-box;\r\n  padding: 0.5em 2em 0.5em 0.5em;\r\n  border: 1px solid #eee;\r\n  font: inherit;\r\n  line-height: inherit;\r\n  -webkit-appearance: none;\r\n  -moz-appearance: none;\r\n  -ms-appearance: none;\r\n  appearance: none;\r\n  background-repeat: no-repeat;\r\n  background-image: linear-gradient(45deg, transparent 50%, currentColor 50%),\r\n    linear-gradient(135deg, currentColor 50%, transparent 50%);\r\n  background-position: right 15px top 1em, right 10px top 1em;\r\n  background-size: 5px 5px, 5px 5px;\r\n  cursor: pointer;\r\n  option {\r\n    cursor: pointer;\r\n  }\r\n}\r\n\r\n.board-container {\r\n  width: 95%;\r\n  height: 100%;\r\n  display: grid;\r\n  grid-template-columns: 1fr 1fr;\r\n  grid-gap: 40px;\r\n}\r\n\r\n.box {\r\n  width: 100%;\r\n  height: 100%;\r\n}\r\n\r\n.fields-container {\r\n  display: inline-grid;\r\n  width: 100%;\r\n  height: 100%;\r\n  display: grid;\r\n  grid-template-columns: 1fr 1fr 1fr 1fr;\r\n  grid-template-rows: 150px 150px 150px 150px;\r\n  grid-column-gap: 15px;\r\n}\r\n\r\n.field {\r\n  width: 120px;\r\n  height: 120px;\r\n  background-color: rgb(109, 107, 107);\r\n  box-shadow: inset 0 0 1em rgb(32, 29, 29), 0 0 1em rgb(48, 48, 49);\r\n  &.active {\r\n    background-color: $white;\r\n    &:hover {\r\n      background-color: rgba(rgb(55, 85, 55), $alpha: 0.2);\r\n      cursor: pointer;\r\n    }\r\n  }\r\n}\r\n\r\n.buttons-container {\r\n  display: flex;\r\n  padding-bottom: 60px;\r\n}\r\n\r\n.button {\r\n  margin: 0 20px;\r\n  padding: 10px 20px;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  font-family: 'Montserrat-Regular', sans-serif;\r\n  font-size: 18px;\r\n  color: rgba($color: $white, $alpha: 0.9);\r\n  border-radius: 10px;\r\n  border: none;\r\n  letter-spacing: 0.5px;\r\n  text-transform: uppercase;\r\n  box-shadow: inset 0 0 1em rgb(32, 29, 29), 0 0 1em rgb(48, 48, 49);\r\n  cursor: pointer;\r\n  transition: 0.5s;\r\n\r\n  &.start {\r\n    background-color: #18ac18;\r\n  }\r\n  &.reset {\r\n    background-color: #8f1c2f;\r\n  }\r\n  &:hover {\r\n    opacity: 0.5;\r\n  }\r\n}\r\n","styles\\scss\\colors.scss":"$primary-color: #222222;\r\n\r\n$white: #eee;\r\n"},"lineCount":null}},"error":null,"hash":"6bb1e105536a8f2feef0864a9896b02f","cacheData":{"env":{}}}